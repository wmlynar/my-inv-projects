#!/usr/bin/env bash
# SEAL E2E defaults (auto-loaded by E2E scripts).
# Copy to .seal/e2e.env to override per-repo without touching this file.

# Docker E2E
SEAL_DOCKER_E2E_REMOTE=1
SEAL_DOCKER_E2E_PULL=0
SEAL_DOCKER_E2E_BUILDER=full
SEAL_DOCKER_E2E_REBUILD=0
SEAL_DOCKER_E2E_CACHE_DIR=/var/tmp/seal-e2e-cache
SEAL_DOCKER_E2E_PROGRESS=plain
SEAL_DOCKER_E2E_SAVE_IMAGE=1
SEAL_E2E_TOOLSET=core

# Share example node_modules across runs to avoid re-install.
SEAL_E2E_USE_SHARED_NODE_MODULES=1

# Parallelism
SEAL_E2E_PARALLEL=1
SEAL_E2E_PARALLEL_MODE=per-test
SEAL_E2E_JOBS=

# Dependency installs (leave empty for auto-detect)
SEAL_E2E_INSTALL_DEPS=
SEAL_E2E_INSTALL_PACKERS=
SEAL_E2E_INSTALL_OBFUSCATORS=
SEAL_E2E_INSTALL_EXAMPLE_DEPS=

# Force rebuilds (set to 1 to recompile)
SEAL_E2E_REINSTALL_PACKERS=0
SEAL_E2E_REINSTALL_OBFUSCATORS=0

# Optional test filters
SEAL_E2E_TESTS=
SEAL_E2E_SKIP=

# Strict anti-debug flags (set to 1 to enforce)
SEAL_E2E_STRICT_PROC_MEM=0
SEAL_E2E_STRICT_PTRACE=0
SEAL_E2E_STRICT_SNAPSHOT_GUARD=0
