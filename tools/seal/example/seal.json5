{
  "appName": "seal-example",
  "entry": "src/index.js",
  "defaultTarget": "local",
  "build": {
    "packager": "thin-split",
    "packagerFallback": false,
    "securityProfile": "strict",
    "thin": {
      "mode": "split",
      "level": "low",
      "chunkSizeBytes": 8388608,
      "zstdLevel": 1,
      "zstdTimeoutMs": 120000,
      "envMode": "allowlist",
      "runtimeStore": "memfd",
      "integrity": {
        "enabled": true,
        "mode": "sidecar",
        "file": "ih"
      },
      "appBind": {
        "enabled": true,
        "value": "seal-example"
      },
      "snapshotGuard": {
        "enabled": true,
        "intervalMs": 1000,
        "maxJumpMs": 60000,
        "maxBackMs": 100
      },
      "launcherHardening": true,
      "launcherHardeningCET": false,
      "launcherObfuscation": true,
      "antiDebug": {
        "enabled": true,
        "tracerPid": true,
        "tracerPidIntervalMs": 10000,
        "tracerPidThreads": true,
        "denyEnv": true,
        "mapsDenylist": [
          "gdb",
          "lldb",
          "strace",
          "ltrace",
          "rr",
          "frida"
        ],
        "ptraceGuard": {
          "enabled": true,
          "dumpable": true
        },
        "seccompNoDebug": {
          "enabled": true,
          "mode": "errno"
        },
        "coreDump": true,
        "loaderGuard": true
      }
    },
    "sentinel": {
      "enabled": true,
      "level": "auto",
      "appId": "auto",
      "namespaceId": "auto",
      "cpuIdSource": "auto",
      "storage": {
        "baseDir": "/var/lib",
        "mode": "file"
      },
      "exitCodeBlock": 200,
      "externalAnchor": {
        "type": "none"
      }
    },
    "frontendObfuscation": {
      "enabled": true
    },
    "protection": {
      "enabled": true,
      "seaMain": {
        "pack": true,
        "method": "brotli",
        "chunkSize": 8000
      },
      "bundle": {
        "pack": true
      },
      "strip": {
        "enabled": true,
        "cmd": "strip",
        "args": [
          "--strip-all"
        ]
      },
      "elfPacker": {
        "tool": "kiteshield",
        "cmd": "kiteshield",
        "args": [
          "-n",
          "{in}",
          "{out}"
        ]
      },
      "strings": {
        "obfuscation": [
          "xorstr",
          "crystr",
          "obfuscate"
        ]
      },
      "cObfuscator": {
        "tool": "obfuscator-llvm",
        "cmd": "ollvm-clang",
        "args": [
          "-mllvm",
          "-fla",
          "-mllvm",
          "-sub",
          "-mllvm",
          "-bcf",
          "-mllvm",
          "-bcf_loop=3"
        ]
      }
    },
    "decoy": {
      "mode": "none",
      "scope": "backend",
      "sourceDir": "decoy",
      "overwrite": false,
      "generator": "off" // off | basic (generate decoy when sourceDir missing)
    },
    "includeDirs": [
      "public",
      "data"
    ],
    "keepLocalOut": true
  },
  "policy": {
    "retention": {
      "keepReleases": 10,
      "cleanupOnSuccess": true,
      "neverDeleteCurrent": true,
      "keepAtLeastOneRollback": true
    }
  }
}
