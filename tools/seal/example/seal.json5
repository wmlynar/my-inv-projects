{
  "appName": "seal-example",
  "entry": "src/index.js",
  "defaultTarget": "local",
  "build": {
    "packager": "auto",
    "bundleFallback": false,
    "obfuscationProfile": "balanced",
    "frontendObfuscation": {
      "enabled": true,
      "profile": "balanced"
    },
    "protection": {
      "enabled": true,
      "packSeaMain": true,
      "packSeaMainMethod": "brotli",
      "packSeaMainChunkSize": 8000,
      "packBundle": true,
      "stripSymbols": false,
      "upxPack": false
    },
    "includeDirs": [
      "public",
      "data"
    ],
    "keepLocalOut": true
  },
  "policy": {
    "retention": {
      "keepReleases": 10,
      "cleanupOnSuccess": true,
      "neverDeleteCurrent": true,
      "keepAtLeastOneRollback": true
    }
  }
}
