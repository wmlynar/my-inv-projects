{
  "appName": "seal-example",
  "entry": "src/index.js",
  "defaultTarget": "local",
  "build": {
    "packager": "auto",
    "packagerFallback": false,
    "obfuscationProfile": "balanced",
    "frontendObfuscation": {
      "enabled": true,
      "profile": "balanced"
    },
    "protection": {
      "enabled": true,
      "seaMain": {
        "pack": true,
        "method": "brotli",
        "chunkSize": 8000
      },
      "bundle": {
        "pack": true
      },
      "strip": {
        "enabled": false,
        "cmd": "strip"
      },
      "upx": {
        "enabled": false
      }
    },
    "includeDirs": [
      "public",
      "data"
    ],
    "keepLocalOut": true
  },
  "policy": {
    "retention": {
      "keepReleases": 10,
      "cleanupOnSuccess": true,
      "neverDeleteCurrent": true,
      "keepAtLeastOneRollback": true
    }
  }
}
