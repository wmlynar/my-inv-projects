{
  appName: "robot-ui",
  entry: "server.js",
  defaultTarget: "local",
  build: {
    packager: "auto",
    allowFallback: false,
    obfuscationProfile: "balanced",
    frontendObfuscation: { enabled: true, profile: "balanced" },
    frontendMinify: { enabled: true, level: "safe", html: true, css: true },
    hardening: { enabled: true, strip: false, upx: false, bundlePacking: true },
    includeDirs: ["public"],
  },
}
