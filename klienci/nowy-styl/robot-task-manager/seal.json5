{
  "appName": "robot-task-manager",
  "entry": "robot-task-manager.js",
  "defaultTarget": "local",
  "build": {
    "packager": "sea",
    "packagerFallback": false,
    "obfuscationProfile": "balanced",
    "frontendObfuscation": {
      "enabled": true,
      "profile": "balanced"
    },
    "frontendMinify": {
      "enabled": true,
      "level": "safe",
      "html": true,
      "css": true
    },
    "protection": {
      "enabled": true,
      "bundle": {
        "pack": true
      },
      "strip": {
        "enabled": false
      }
    },
    "includeDirs": [
      "public"
    ]
  },
  "policy": {
    "retention": {
      "keepReleases": 1,
      "cleanupOnSuccess": true,
      "neverDeleteCurrent": true,
      "keepAtLeastOneRollback": true
    }
  }
}
